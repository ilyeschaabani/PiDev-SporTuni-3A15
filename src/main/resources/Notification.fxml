<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" styleClass="gray-bg" stylesheets="@CSS/Stat.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Notification">
    <!-- Labels pour afficher les informations sur le cours -->
    <Label style="-fx-font-weight: bold; -fx-font-size: 16px;" styleClass="nom" stylesheets="@CSS/AffichageCour.css" text="Historique du cour :" textFill="#f4f2f2">
        <font>
            <Font name="Bodoni MT Bold" size="12.0" />
        </font></Label>


    <ListView fx:id="notificationsListView" prefHeight="319.0" prefWidth="600.0" />

</VBox>
